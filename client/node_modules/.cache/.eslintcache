 useHistory();\n    \n    useEffect(() => {\n        axios.get('http://localhost:8000/api/pets/' +id)\n            .then(res => setPet(res.data))\n            .catch(err => console.error(err));\n    }, []);\n    \n    return (\n        <div>\n            <div className=\"d-flex justify-content-between m-4\">\n                <h1>Details about: {pet.name}</h1>\n            <AdoptButton petId={pet._id} petName={pet.name} successCallback={()=>history.push('/')}/>\n            </div>\n            <div className=\"\">\n                <table className=\"table customTable\">\n                    <tbody>\n                        <tr>\n                            <th>Name</th>\n                                <td>{pet.name}</td>\n                        </tr>\n                        <tr>\n                            <th>Type</th>\n                                <td>{pet.type}</td>\n                        </tr>\n                        <tr>\n                            <th>Description</th>\n                                <td>{pet.description}</td>\n                        </tr>\n                        <tr>\n                            <th>Skills</th>\n                            <td>\n                                <ul className=\"list-group\">\n                                    <li className=\"list-group-item border-0\">{pet.skill1}</li>\n                                    <li className=\"list-group-item border-0\">{pet.skill2}</li>\n                                    <li className=\"list-group-item border-0\">{pet.skill3}</li>\n                                </ul>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n                <LikeButton/>\n            </div>\n        </div>\n    )\n}\n    \nexport default Detail;\n\n","/Users/macrock/Desktop/CodingDojo/MERNStack/Exam/pet-shelter/client/src/views/Main.js",["327","328"],[],"import React, { useEffect, useState } from 'react'\nimport axios 